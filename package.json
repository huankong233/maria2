{
  "name": "maria2",
  "version": "0.1.2",
  "description": "Simple RPC Library for aria2",
  "keywords": [
    "aria2",
    "download",
    "http",
    "websocket",
    "metalink",
    "magnet",
    "ftp",
    "bittorrent",
    "typescript"
  ],
  "homepage": "https://github.com/hydrati/maria2#readme",
  "bugs": {
    "url": "https://github.com/hydrati/maria2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydrati/maria2.git"
  },
  "author": {
    "name": "Hydration",
    "url": "https://github.com/hydrati"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "format": "prettier --write src/**/*.ts",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "preversion": "pnpm typecheck && pnpm build",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.6.7",
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@types/ws": "^8.5.5",
    "bumpp": "^9.1.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.4.2",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "isomorphic-ws": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  }
}
